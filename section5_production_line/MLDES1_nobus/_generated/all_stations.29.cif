  controllable c_on;
  controllable c_off;
  location On:
    edge c_off goto Off;
  location Off:
    initial;
    marked;
    edge c_on goto On;
end
plant automaton HDynamics2:
  location:
    initial;
    marked;
    edge S_transfer_atpickup.u_on when S_transfer_atdrop.Off and S_transfer_athalfway.Off;
    edge S_transfer_atdrop.u_on when S_transfer_atpickup.Off and S_transfer_athalfway.Off;
    edge S_transfer_athalfway.u_on when S_transfer_atdrop.Off and S_transfer_atpickup.Off;
end
requirement invariant Stations12Req54: A_transfer2pickpos.c_on needs S_Hinitialized.On;
requirement invariant Stations12Req55: A_transfer2pickpos.c_off needs S_Hinitialized.On;
requirement invariant Stations12Req56: A_transfer2droppos.c_on needs S_Hinitialized.On;
requirement invariant Stations12Req57: A_transfer2droppos.c_off needs S_Hinitialized.On;
requirement invariant Stations12Req58: A_vacuum.c_on needs S_Hinitialized.On;
requirement invariant Stations12Req59: A_vacuum.c_off needs S_Hinitialized.On;
requirement invariant Stations12Req60: A_ejectpulse.c_on needs S_Hinitialized.On;
requirement invariant Stations12Req61: A_ejectpulse.c_off needs S_Hinitialized.On;
requirement invariant Stations12Req79: A_transfer2pickpos.c_on needs A_transfer2droppos.Off;
requirement invariant Stations12Req80: A_transfer2droppos.c_on needs A_transfer2pickpos.Off;
requirement invariant Stations12Req81: A_transfer2pickpos.c_on needs S_vacuum.Off;
requirement invariant Stations12Req82: A_transfer2droppos.c_on needs S_vacuum.On;
